{
  "name": "home-mgmt",
  "version": "0.0.0",
  "description": "Home Management",
  "main": "server/app.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others \"ng serve --proxy-config proxy.conf.json\" \"npm run _server:run\"",
    "build": "ng build --prod --output-path=dist/client && npm run _server:build",
    "_server:build": "tsc -p ./server",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build"
  },
  "author": "Sanjeev Tripathi",
  "engines": {
    "node": "7.7.2",
    "npm": "4.4.1"
  },
  "repository": {
    "url": " https://git.heroku.com/homemgmt.git"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "@types/auth0-js": "^8.3.0",
    "@types/auth0-lock": "^10.10.0",
    "@types/body-parser": "^1.16.0",
    "@types/compression": "0.0.33",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.35",
    "@types/youtube": "0.0.28",
    "angular2-jwt": "^0.1.28",
    "auth0-js": "^8.3.0",
    "auth0-lock": "^10.12.3",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "express": "^4.15.2",
    "mongojs": "^2.4.0",
    "ng2-youtube-player": "0.0.1",
    "path": "^0.12.7",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.1",
    "@angular/compiler-cli": "^2.4.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "~2.0.0"
  }
}
